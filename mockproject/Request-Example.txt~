Description: Create new appraiser
POST: http://localhost:8890/appraiser/new
{
    "name": "John Doe",
    "email": "johndoe@example.com",
    "phone": "123-456-7890",
    "status": "Active",
    "type": "Independent",
    "companyName": "Doe Appraisal Services",
    "taxNumber": "TAX-123456",
    "experience": 5,
    "address": "123 Main Street, New York, NY"
}
